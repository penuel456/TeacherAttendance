<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_editschedule"
        tools:context=".activity_editschedule">

    <TextView
            android:text="Building"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5" android:layout_marginTop="72dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="56dp" android:layout_marginStart="56dp"/>
    <TextView
            android:text="Room"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="56dp"
            android:layout_marginStart="56dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <TextView
            android:text="Start Time"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:id="@+id/textView7" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="56dp"
            android:layout_marginStart="56dp" android:layout_marginTop="40dp"
            app:layout_constraintTop_toBottomOf="@+id/textView6"/>
    <TextView
            android:text="End Time"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:id="@+id/textView8" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="56dp" android:layout_marginStart="56dp"/>
    <TextView
            android:text="Day"
            android:layout_width="wrap_content"
            android:layout_height="21dp"
            android:id="@+id/textView9" app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="56dp"
            android:layout_marginStart="56dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView8"/>
    <EditText
            android:layout_width="209dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:text="Input the building..."
            android:ems="10"
            android:id="@+id/editText" android:layout_marginTop="52dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginEnd="80dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="80dp"/>
    <EditText
            android:layout_width="209dp"
            android:layout_height="43dp"
            android:inputType="textPersonName"
            android:text="Input the room..."
            android:ems="10"
            android:id="@+id/editText2"
            app:layout_constraintTop_toBottomOf="@+id/editText" android:layout_marginEnd="80dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="80dp"/>
    <Spinner
            android:layout_width="168dp"
            android:layout_height="22dp"
            android:id="@+id/spinner2" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="108dp"
            android:layout_marginRight="108dp" android:layout_marginTop="28dp"
            app:layout_constraintTop_toBottomOf="@+id/editText2"/>
    <Spinner
            android:layout_width="168dp"
            android:layout_height="22dp"
            android:id="@+id/spinner3" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="108dp"
            android:layout_marginRight="108dp" android:layout_marginTop="12dp"
            app:layout_constraintTop_toBottomOf="@+id/spinner2"/>
    <Spinner
            android:layout_width="168dp"
            android:layout_height="22dp"
            android:id="@+id/spinner4" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="108dp"
            android:layout_marginRight="108dp" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/spinner3"/>
</android.support.constraint.ConstraintLayout>