<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             android:background="@android:color/background_light"
                                             android:id="@+id/relativeLayout">

    <RadioGroup
            android:layout_width="125dp"
            android:layout_height="168dp"
            android:id="@+id/smed_wings"
            app:layout_constraintHorizontal_bias="0.716" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/safad_floors"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            android:visibility="invisible"
            android:layout_marginEnd="40dp" android:layout_marginRight="40dp" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView2">
        <RadioButton
                android:text="RadioButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/smed_hiddenwing" android:layout_weight="1"
                android:visibility="gone" android:buttonTint="#0ba156"/>
        <RadioButton
                android:text="RadioButton"
                android:layout_width="wrap_content"
                android:layout_height="51dp" tools:layout_editor_absoluteY="50dp"
                tools:layout_editor_absoluteX="174dp" android:id="@+id/smed_wing1" android:buttonTint="#0ba156"/>
        <RadioButton
                android:text="RadioButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/smed_wing2" android:layout_weight="1"
                android:buttonTint="#0ba156"/>
        <RadioButton
                android:text="RadioButton"
                android:layout_width="wrap_content"
                android:layout_height="19dp" android:id="@+id/smed_wing3" android:layout_weight="1"
                android:buttonTint="#0ba156"/>
    </RadioGroup>
    <RadioGroup
            android:layout_width="113dp"
            android:layout_height="180dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            android:layout_marginTop="24dp" app:layout_constraintHorizontal_bias="0.102" android:id="@+id/smed_floors"
            app:layout_constraintTop_toBottomOf="@+id/textView2">
        <RadioButton
                android:text="1st Floor"
                android:layout_width="wrap_content"
                android:layout_height="45dp" android:id="@+id/firstFloorBtn" android:layout_weight="1"
                android:buttonTint="#0ba156"
                tools:text="1st Floor"/>
        <RadioButton
                android:text="2nd Floor"
                android:layout_width="wrap_content"
                android:layout_height="43dp" android:id="@+id/secondFloorBtn" android:layout_weight="1"
                android:buttonTint="#0ba156"
                tools:text="2nd Floor"/>
        <RadioButton
                android:text="3rd Floor"
                android:layout_width="wrap_content"
                android:layout_height="46dp" android:id="@+id/thirdFloorBtn" android:layout_weight="1"
                android:buttonTint="#0ba156"
                tools:text="3rd Floor"/>
    </RadioGroup>
    <TextView
            android:text="Choose a Floor"
            android:layout_width="167dp"
            android:layout_height="27dp" android:id="@+id/textView2"
            android:layout_marginLeft="36dp" android:layout_marginStart="36dp"
            android:layout_marginTop="216dp"
            android:layout_marginBottom="633dp" tools:textSize="18sp"
            android:textColor="@android:color/background_dark" tools:text="Choose a Floor"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintVertical_bias="0.0" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.005" android:textSize="24sp" android:textStyle="bold"/>
    <TextView
            android:text="Choose a Wing"
            android:layout_width="167dp"
            android:layout_height="27dp" android:id="@+id/textView14"
            android:layout_marginLeft="36dp" android:layout_marginStart="36dp"
            android:layout_marginTop="216dp"
            android:layout_marginBottom="633dp" tools:textSize="18sp"
            android:textColor="@android:color/background_dark" tools:text="Choose a Wing"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintVertical_bias="0.0" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="0.84" android:textSize="24sp" android:textStyle="bold"
            android:visibility="gone"/>
    <ImageView
            android:layout_width="412dp"
            android:layout_height="206dp" app:srcCompat="@drawable/smed"
            android:id="@+id/imageView8" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
</android.support.constraint.ConstraintLayout>
